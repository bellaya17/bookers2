<h2>User info</h2>
      <thead>
        <tr>
         <%= attachment_image_tag user, :profile_image, :fill, 100, 100, fallback: "no_image.jpg"%>
        </tr>
      </thead>

      <table class="table">
        <tbody>
          <tr>
            <th>name</th>
            <th><%= user.name %></th>
          </tr>
          <tr>
            <th>introduction</th>
            <th><%= user.introduction %></th>
          </tr>
      </tbody>
      </table>
      <div class="row">
          <%= link_to edit_user_path(@user), class: "btn btn-outline-secondary btn-block" do %>
            <i class="fas fa-user-cog"></i>
          <% end %>
         <!--<a class="btn btn-outline-secondary btn-block fas fa-user-cog" href= "/users/edit_user_path(user)/edit" ></a>-->

      </div>



